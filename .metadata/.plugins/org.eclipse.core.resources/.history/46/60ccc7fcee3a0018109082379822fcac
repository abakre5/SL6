import java.util.ArrayList;
import java.util.List;


public class Cluster {

	public List<Point> points;
	public Point centroid;
	int id;
	static int ids = 0;
	
	
	public Cluster(){
		this.id = ids++;
		this.centroid = null;
		this.points = new ArrayList<Point>();
	}
	
	public void Clear(){
		points.clear();
	}
	
	
	
}
